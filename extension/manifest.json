{
    "name": "CoffeeSpec Helper",
    "version": "0.1",
    "manifest_version": 2,
    "description": "A coffee spec helper!",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png" },
    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["http://coffeespec.dev/*", "http://coffeespec.heroku.com/*"],
        "js": ["jquery.js", "coffeespec.js"],
        "run_at": "document_end"
    }],
    "background": {
        "scripts": ["background.js"]
    }
}