{
    "name": "CoffeeSpec Helper",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Help your coffee!",
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png" },
    "browser_action": {
        "default_icon": "assets/icon19.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["coffespec.js"],
        "run_at": "document_end"
    }]
}